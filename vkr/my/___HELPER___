python manage.py seed my --number=10

=== ENTER_MANAGE_SHELL ===
python manage.py shell

=== CUSTOM_SEEDERS ===
import random
from django_seed import Seed
from django.contrib.auth import get_user_model 
from my.models import *
seeder = Seed.seeder()
# print(ToDoItem.objects.filter(id=random.choice(range(1, 50))).first()) # example for random record in range
seeder.add_entity(Person, 100)

seeder.add_entity(Winner, 5, {
    'game':    Game.objects.filter(uid='76a335c8-7cfe-4a42-bfdf-d05f903f2085').first(),
    'sport': Sport.objects.filter(id=random.choice(range(1, 5))).first(),
    'person': Person.objects.filter(id=random.choice(range(1, 110))).first()
})
seeder.execute()

=== UPDATE_DATA ===
from django.db import connection
Game.objects.update(date_to='2024-01-01')
get_user_model().objects.update(password='secret')
POSTGRESQL: sql = "UPDATE my_game SET date_to = (date_from + INTERVAL '30 day')" 
SQLITE: sql = "update news set updated_at  = datetime(created_at, '+600 seconds')"

sql = "UPDATE my_person SET born = (born - INTERVAL '18 year') WHERE born > '2010-01-01'"
connection.cursor().execute(sql)

